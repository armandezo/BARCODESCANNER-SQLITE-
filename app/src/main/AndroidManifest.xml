<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.barcode.scanner">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.app.barcode.scanner.SplashScreen"
            android:label="@string/app_name"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.app.barcode.scanner.MainActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="com.app.barcode.scanner.Menu"
            android:label="@string/app_name" />
        <activity
            android:name="com.app.barcode.scanner.Customer.CustomerView"
            android:label="@string/app_name" />
        <activity
            android:name="com.app.barcode.scanner.Customer.Scanner"
            android:label="@string/app_name" />
        <activity
            android:name="com.app.barcode.scanner.Customer.TambahCustomer"
            android:label="@string/app_name" />
        <activity
            android:name="com.app.barcode.scanner.Scan.MainScan"
            android:label="@string/app_name" />
        <activity
            android:name="com.app.barcode.scanner.Scan.ScanKeluar"
            android:label="@string/app_name" />
        <activity
            android:name="com.app.barcode.scanner.Scan.ScanMasuk"
            android:label="@string/app_name" />
        <activity
            android:name="com.app.barcode.scanner.Admin.Admin"
            android:label="@string/app_name" />
        <activity
            android:name="com.app.barcode.scanner.Admin.BuatAdmin"
            android:label="@string/app_name" />
        <activity
            android:name="com.app.barcode.scanner.Admin.LihatAdmin"
            android:label="@string/app_name" />
        <activity
            android:name="com.app.barcode.scanner.Admin.UpdateAdmin"
            android:label="@string/app_name" />
        <activity android:name="com.app.barcode.scanner.Histori" />
        <activity android:name="com.app.barcode.scanner.LihatHistori" />
    </application>

</manifest>